@model POYA.Models.LUserFile

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@(string.Format(Localizer["Details for {0}"],Model.Name)) </h1>

<partial name="_DetailsPartial" model="Model" />


<div>
    <a asp-action="Index" asp-route-InDirId="@(Model.InDirId)" class="btn btn-outline-info"> << @Localizer["Back"]</a>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-outline-primary">@Localizer["Edit"]</a>
</div>

@section Scripts{
    <script>
    </script>
}

@section Styles{
    <style>
        video,img {
            max-width: 90%;
        }
    </style>
}